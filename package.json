{
  "name": "litellm-proxy-lambda",
  "version": "1.0.0",
  "description": "LiteLLM Proxy deployment on AWS Lambda with Docker",
  "packageManager": "pnpm@9.6.0",
  "scripts": {
    "sls": "serverless",
    "deploy": "pnpm sls deploy --verbose",
    "deploy:function": "pnpm sls deploy function -f proxy",
    "logs": "pnpm sls logs -f proxy --tail",
    "remove": "pnpm sls remove",
    "info": "pnpm sls info",
    "install:deps": "pnpm install --frozen-lockfile"
  },
  "devDependencies": {
    "serverless": "^4.4.3"
  },
  "keywords": [
    "litellm",
    "proxy",
    "lambda",
    "aws",
    "docker",
    "openai",
    "anthropic"
  ],
  "author": "",
  "license": "MIT"
}